{
  "name": "mirzagram",
  "version": "1.5.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev": "npx expo start --dev-client",
    "preBuild": "npx expo prebuild --clean",
    "deBuild": "npx expo run:android --device",
    "apkBuild": "eas build -p android --profile preview",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watch",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|AsyncStorage)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/slider": "^4.5.2",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo": "~51.0.24",
    "expo-asset": "~10.0.9",
    "expo-av": "~14.0.6",
    "expo-blur": "~13.0.2",
    "expo-checkbox": "~3.0.0",
    "expo-constants": "~16.0.2",
    "expo-contacts": "~13.0.5",
    "expo-dev-client": "~4.0.21",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image-picker": "~15.0.7",
    "expo-intent-launcher": "~11.0.1",
    "expo-media-library": "~16.0.4",
    "expo-notifications": "~0.28.14",
    "expo-splash-screen": "~0.27.5",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.1",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "lottie-react-native": "6.7.0",
    "nanoid": "^5.0.1",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-drawer-layout": "^3.3.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-lightbox-v2": "^0.9.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-outside-press": "^1.2.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-theme-switch-animation": "^0.8.0",
    "react-native-toast-message": "^2.1.8",
    "react-native-track-player": "^4.1.1",
    "socket.io-client": "^4.7.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.79",
    "typescript": "~5.3.3"
  },
  "private": true
